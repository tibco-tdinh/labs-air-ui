{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "on-prem": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/on-prem",
      "sourceRoot": "projects/on-prem/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../../dist/ProjectAir",
            "index": "projects/on-prem/src/index.html",
            "main": "projects/on-prem/src/main.ts",
            "polyfills": "projects/on-prem/src/polyfills.ts",
            "tsConfig": "projects/on-prem/tsconfig.app.json",
            "assets": [
              "projects/on-prem/src/favicon.ico",
              "projects/on-prem/src/assets"
            ],
            "styles": [
              "projects/on-prem/src/assets/geoanalytics/GeoAnalytics.css",
              "projects/on-prem/src/styles.css"
            ],
            "scripts": [
              "projects/on-prem/src/assets/geoanalytics/GeoAnalytics.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/on-prem/src/environments/environment.ts",
                  "with": "projects/on-prem/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "on-prem:build:production"
            },
            "development": {
              "browserTarget": "on-prem:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "on-prem:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "codeCoverage": true,
            "polyfills": "projects/on-prem/src/polyfills.ts",
            "tsConfig": "projects/on-prem/tsconfig.spec.json",
            "karmaConfig": "projects/on-prem/karma.conf.js",
            "assets": [
              "projects/on-prem/src/favicon.ico",
              "projects/on-prem/src/assets"
            ],
            "styles": [
              "projects/on-prem/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/on-prem/src/**/*.ts",
              "projects/on-prem/src/**/*.html"
            ]
          }
        }
      }
    },
    "cloud": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/cloud",
      "sourceRoot": "projects/cloud/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../../dist/ProjectAir",
            "index": "projects/cloud/src/index.html",
            "main": "projects/cloud/src/main.ts",
            "polyfills": "projects/cloud/src/polyfills.ts",
            "tsConfig": "projects/cloud/tsconfig.app.json",
            "assets": [
              "projects/cloud/src/favicon.ico",
              "projects/cloud/src/assets"
            ],
            "styles": [
              "projects/cloud/src/assets/geoanalytics/GeoAnalytics.css",
              "projects/cloud/src/styles.css"
            ],
            "scripts": [
              "projects/cloud/src/assets/geoanalytics/GeoAnalytics.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/cloud/src/environments/environment.ts",
                  "with": "projects/cloud/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "cloud:build:production"
            },
            "development": {
              "browserTarget": "cloud:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cloud:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/cloud/src/test.ts",
            "codeCoverage": true,
            "polyfills": "projects/cloud/src/polyfills.ts",
            "tsConfig": "projects/cloud/tsconfig.spec.json",
            "karmaConfig": "projects/cloud/karma.conf.js",
            "assets": [
              "projects/cloud/src/favicon.ico",
              "projects/cloud/src/assets"
            ],
            "styles": [
              "projects/cloud/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/cloud/src/**/*.ts",
              "projects/cloud/src/**/*.html"
            ]
          }
        }
      }
    },
    "common": {
      "projectType": "library",
      "root": "projects/common",
      "sourceRoot": "projects/common/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/common/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/common/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "main": "projects/common/src/test.ts",
            "tsConfig": "projects/common/tsconfig.spec.json",
            "karmaConfig": "projects/common/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/common/**/*.ts",
              "projects/common/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "on-prem",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
